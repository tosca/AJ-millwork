<body>
    <!-- Top Bar-->
    <div id="top-bar" class="top-bar-section top-bar-semi-transparent">
        <div class="container">
            <div class="row">
                <div class="col-sm-12">
                    <div class="top-contact top-contact-right link-hover-black">
                         <a href="tel:16306597574">
                              <i class="fa fa-phone"></i> 630.659.7574
                           </a>
                           <a href="mailto:a-jmillwork.com">
                                <i class="fa fa-envelope"></i> a-jmillwork.com
                            </a>
                     </div>
                </div>
            </div>
        </div>
    </div> 
  <div class="transparent-header">
    <!-- Sticky Navbar -->
    <header id="sticker" class="sticky-navigation header-logo-center">
        <!-- Sticky Menu -->
        <div class="sticky-menu relative">
            <!-- navbar -->
            <div class="navbar navbar-default navbar-bg-transparent" role="navigation">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="navbar-header">
                                <!-- Button For Responsive toggle -->
                                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                                    <span class="sr-only">Toggle navigation</span>
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                </button>
                                <a class="navbar-brand" href="index.html">
                                    <img class="site_logo" alt="Site Logo" src="~/img/ajLogo-sm.png" />
                                </a>  </div>
                            <!-- Navbar Collapse -->
                            <div class="navbar-collapse collapse">
                                <!-- nav -->
                                <ul class="nav navbar-nav">
                                    <!-- Home  Mega Menu -->
                                    <li class="mega-menu">
                                        <a href="~/AJMillwork">Home</a>
                                    </li>
                                    <li class="mega-menu">
                                        <a href="~/Portfolio">Portfolio</a>
                                    </li>
                                    <li class="mega-menu">
                                        <a href="~/ArchitecturalMillwork">Architectural Millwork</a>
                                    </li>
                                    <li class="mega-menu">
                                        <a href="#contact-us">Contact</a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        </header>
</div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>

          
            <section id="contact-us" class="page-section">
                <div class="image-bg content-in fixed" data-background="img/stair/stair-32.jpg">
                    <div class="overlay"></div>
                </div>
                <div class="container">
                    <div class="row">
                        <div class="col-md-6 text-center">
                            <h2 class="text-color">Contact Us</h2>

                            <p class="white">
                                AJ Millwork, Inc. provides outstanding quality and exceptional value. From production to installation, we work toward our client's satisfaction.
                            </p>
                            <hr class="white" />
                            <h4 class="white">630-659-7574</h4>
                            <h3 class="text-color">Keith Czaja</h3>
                            <h4> 
                                <a class="text-color" href="contact@a-jmillwork.com">contact@a-jmillwork.com</a>
                            </h4>
                            <hr class="white" />

                        </div>
                        <div class="col-md-6">
                            <p class="form-message" style="display: none;"></p>
                            <div class="contact-form field-border-bottom">
                                <!-- Form Begins -->
                                <form role="form" name="contactform" id="contactform" method="post" action="php/contact-form.php">
                                    <!-- Field 1 -->
                                    <div class="input-text form-group">
                                        <input type="text" name="contact_name" class="input-name form-control" placeholder="Full Name" />
                                    </div>
                                    <!-- Field 2 -->
                                    <div class="input-email form-group">
                                        <input type="email" name="contact_email" class="input-email form-control" placeholder="Email" />
                                    </div>
                                    <!-- Field 3 -->
                                    <div class="input-email form-group">
                                        <input type="text" name="contact_phone" class="input-phone form-control" placeholder="Phone" />
                                    </div>
                                    <!-- Field 4 -->
                                    <div class="textarea-message form-group">
                                        <textarea name="contact_message" class="textarea-message hight-82 form-control" placeholder="Message" rows="2"></textarea>
                                    </div>
                                    <!-- Button -->
                                    <button class="btn btn-default btn-block" type="submit">
                                        Send Now
                                        <i class="icon-paper-plane"></i>
                                    </button>
                                </form>
                                <!-- Form Ends -->
                            </div>
                        </div>
                    </div>
                </div>
            </section>

   <p>&copy; @DateTime.Now.Year - AJ Millwork, Inc.</p>
        </footer>
    </div>

   @Scripts.Render("~/bundles/jquery")
   @Scripts.Render("~/bundles/bootstrap")

