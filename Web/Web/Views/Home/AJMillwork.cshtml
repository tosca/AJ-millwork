@{
    ViewBag.Title = "AJ MILLWORK";
}


<section class="slider" style="background:rgb(0,0,0);">

    <div class="tp-banner">
        <ul>
            <li data-delay="3000" data-transition="fade" data-slotamount="7" data-masterspeed="1000">
                <img src="~/img/slider/slide-20.jpg" alt="AJ Millwork kitchen" data-bgfit="contain" data-bgposition="center top" data-bgrepeat="no-repeat" />
            </li>
            <li data-delay="3000" data-transition="fade" data-slotamount="7" data-masterspeed="5000">
                <img src="~/img/slider/slide-03.jpg" alt="AJ Millwork contemprary Kitchen counter" data-bgfit="contain" data-bgposition="center top" data-bgrepeat="no-repeat" />
            </li>
            <li data-delay="3000" data-transition="fade" data-slotamount="7" data-masterspeed="5000">
                <img src="~/img/slider/slide-04.jpg" alt="Curved entertainment bar in basement" data-bgfit="contain" data-bgposition="center top" data-bgrepeat="no-repeat" />
            </li>
            <li data-delay="3000" data-transition="fade" data-slotamount="7" data-masterspeed="5000">
                <img src="~/img/slider/slide-02.jpg" alt="Stair well" data-bgfit="contain" data-bgposition="center top" data-bgrepeat="no-repeat" />
            </li>
        </ul>
       <div class="tp-bannertimer"></div>
    </div>
</section>
