@{
    ViewBag.Title = "DesignerFurniture";
}
 
<section id="designer-furniture" class="page-section">
   <div class="container">
            <div class="row text-center bottom-pad-20">
                <div class="col-sm-12 col-md-12">
                    <div class="section-title">
                        <h2 class="title">Designer Furniture</h2>
                    </div>
                </div>
            </div>
     
            <div class="row tb-margin-50">
                <div class="mixed-grid pad">
                    <div class="masonry-grid grid-col-3" data-gutter="20">
                        <div class="grid-sizer"></div>
                        <div class="grid-item all design-items">
                            <a class="venobox" data-gall="portfolio-gallery" href="~/img/entertain/furniture-02.jpg">
                                <img src="~/img/entertain/furniture-02.jpg" alt="Millwork" class="img-responsive border3" />
                            </a>
                        </div>
                        <div class="grid-item all">
                            <a class="venobox" data-gall="portfolio-gallery" href="~/img/entertain/furniture-04.jpg">
                                <img src="~/img/entertain/furniture-04.jpg" alt="Millwork" class="img-responsive border3" />
                            </a>
                        </div>
                        <div class="grid-item all fade-right">
                            <a class="venobox" data-gall="portfolio-gallery" href="~/img/entertain/furniture-01.jpg">
                                <img src="~/img/entertain/furniture-01.jpg" alt="Millwork" class="img-responsive border3" />
                            </a>
                        </div>
                    </div>
                </div>
            </div>
            <hr />  
       
  </div>       
</section>
